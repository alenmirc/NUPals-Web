{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Content-Security-Policy", "value": "default-src 'self' https://nupals-web.onrender.com; script-src 'self' https://nupals-web.onrender.com; connect-src 'self' https://nupals-web.onrender.com;" },
          { "key": "X-Frame-Options", "value": "DENY" },
          { "key": "X-Content-Type-Options", "value": "nosniff" },
          { "key": "Referrer-Policy", "value": "no-referrer" },
          { "key": "Permissions-Policy", "value": "geolocation=(), microphone=()" }
        ]
      }
    ]
  }
  